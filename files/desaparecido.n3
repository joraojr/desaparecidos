@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix des: <http://localhost:8890/rdfschema/schemas/desaparecido#> .

des:Desaparecido a rdfs:Class ;
  rdfs:label "Desaparecido" ;
  rdfs:comment "Um Desaparecido" .

des:id a rdf:Property ;
  rdfs:domain des:Desaparecido ;
  rdfs:range xsd:string ;
  rdfs:label "Identificador do desaparecido" .

des:data_desaparecimento a rdf:Property;
  rdfs:domain des:Desaparecido ;
  rdfs:range xsd:string ;
  rdfs:label "Data do desaparecimento" .
